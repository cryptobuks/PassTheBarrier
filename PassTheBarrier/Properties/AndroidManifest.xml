<?xml version="1.0" encoding="utf-8"?>

<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="PassTheBarrier.PassTheBarrier"
          android:versionCode="1" android:versionName="1.0" android:installLocation="auto">
  <uses-sdk android:minSdkVersion="22" android:targetSdkVersion="23" />
  <uses-permission android:name="android.permission.RECEIVE_SMS" />
  <uses-permission android:name="android.permission.READ_SMS" />
  <uses-permission android:name="android.permission.CALL_PHONE" />
  <application android:allowBackup="true" android:label="@string/app_name" android:icon="@drawable/barrier_icon4"
               android:theme="@style/PassTheBarrier.Main">
    <service android:name="passTheBarrier.services.smsService" android:enabled="true">
    </service>
    <receiver android:name="passTheBarrier.broadcastReceivers.serviceRestartReceiver" android:enabled="true">
      <intent-filter>
        <!--	      <action android:name="android.intent.action.BOOT_COMPLETED" />-->
        <action android:name="passTheBarrier.intents.serviceRestart" />
      </intent-filter>
    </receiver>
  </application>
</manifest>